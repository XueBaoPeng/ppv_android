<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 背景图 -->
    <item android:id="@+android:id/background" android:drawable="@drawable/play_process_bg" />
    
    <!-- 第二进度图 -->
    <item android:id="@+id/SecondaryProgress">
        <clip>
            <nine-patch android:src="@drawable/play_progress_loading" />
        </clip>
    </item>
    <!-- 进度度 -->
    <item android:id="@+android:id/progress">
         <clip>
            <nine-patch android:src="@drawable/play_progress" />
        </clip>
    </item>
</layer-list>
